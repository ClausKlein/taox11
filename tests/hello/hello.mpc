// -*- MPC -*-

project(*x11_Idl): ridl_ostream_defaults {
  idlflags += -Sp
  idlflags -= -Sa -St
  IDL_Files {
    test.idl
  }
  custom_only = 1
}

project(*x11_Server): taox11_server {
  after += *x11_Idl
  Source_Files {
    hello.cpp
    server.cpp
  }
  Source_Files {
    testC.cpp
    testS.cpp
  }
}

project(*x11_Client): taox11_client {
  after += *x11_Idl
  Source_Files {
    client.cpp
  }
  Source_Files {
    testC.cpp
  }
}

