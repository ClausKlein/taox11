// -*- MPC -*-

project(*map_idl): ridl_ostream_defaults {
  IDL_Files {
    test.idl
    idlflags += --idl-version=4
  }
  custom_only = 1
}

project(*map_server): taox11_server {
  after += *map_idl
  Source_Files {
    foo.cpp
    server.cpp
  }
  Source_Files {
    testC.cpp
    testS.cpp
  }
}

project(*map_client): taox11_client {
  after += *map_idl
  Source_Files {
    client.cpp
    test_sequences.cpp
    test_corba_sequences.cpp
    test_bounded_sequences.cpp
  }
  Source_Files {
    testC.cpp
  }
}

