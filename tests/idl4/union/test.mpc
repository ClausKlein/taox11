// -*- MPC -*-

project(*idl4_union_gen_idl): ridl_ostream_defaults {
  IDL_Files {
    test.idl
    idlflags += --idl-version=4
  }
  custom_only = 1
}

project(*idl4_union_client): taox11_client {
  after += *idl4_union_gen_idl
  Source_Files {
    client.cpp
  }
  Source_Files {
    testC.cpp
  }
}

