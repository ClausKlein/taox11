union TestUnion_Octet switch(octet) {
  case 1:
    @default(5) short FirstCase;
  case 2:
    long SecondCase;
};

union TestUnion_WChar switch(wchar) {
  case L'X':
    short FirstCase;
  case L'\\':
    long SecondCase;
};
