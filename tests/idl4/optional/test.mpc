// -*- MPC -*-

project(*optional_gen_idl): ridl_ostream_defaults {
  IDL_Files {
    test.idl
    idlflags += --idl-version=4 -Scdr
  }
  custom_only = 1
}

project(*optional_client): taox11_client {
  after += *optional_gen_idl
  Source_Files {
    client.cpp
  }
  Source_Files {
    testC.cpp
  }
}

