// -*- MPC -*-

project(*map_gen_idl): ridl_ostream_defaults {
  IDL_Files {
    test.idl
    idlflags += --idl-version=4
  }
  custom_only = 1
}

project(*map_gen_Server): taox11_server {
  after += *map_gen_idl
  Source_Files {
    foo.cpp
    server.cpp
  }
  Source_Files {
    testC.cpp
    testS.cpp
  }
}

project(*map_client): taox11_client {
  after += *map_gen_idl
  Source_Files {
    client.cpp
  }
  Source_Files {
    testC.cpp
  }
}

