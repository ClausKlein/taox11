// -*- MPC -*-

project(*proxysource_gen_Idl): ridl_ostream_defaults {
  idlflags += -Gcpc -Gatc -Gath -Wb,base_export_macro=TYPELIB -Wb,base_export_include=TYPELIB -Gxhat -Gxhcp
  idlflags -= -Sa -St
  IDL_Files {
    ../test.idl
  }
  custom_only = 1
}

project(*proxysource_taox11_Client): taox11_client, taox11_anytypecode {
  after += *proxysource_gen_Idl
  Source_Files {
    client.cpp
  }
  Source_Files {
    testC.cpp
    testA.cpp
    testCP.cpp
  }
}


