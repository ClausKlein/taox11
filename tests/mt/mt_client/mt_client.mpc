// -*- MPC -*-

project(*idl): ridl_ostream_defaults {
  IDL_Files {
    test.idl
  }
  custom_only = 1
}

project(*server): taox11_server {
  after += *idl
  Source_Files {
    test_i.cpp
    server.cpp
  }
  Source_Files {
    testC.cpp
    testS.cpp
  }
}

project(*client): taox11_client {
  after += *idl
  Source_Files {
    client.cpp
  }
  Source_Files {
    testC.cpp
  }
}
