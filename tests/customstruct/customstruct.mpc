// -*- MPC -*-

project(*hello_gen_Idl): ridl_ostream_defaults {
  idlflags -= -St
  IDL_Files {
    test.idl
  }
  custom_only = 1
}

project(*hello_gen_Server): taox11_server, taox11_anytypecode {
  after += *hello_gen_Idl
  Source_Files {
    hello.cpp
    server.cpp
  }
  Source_Files {
    testC.cpp
    testS.cpp
  }
}

project(*hello_gen_Client): taox11_client, taox11_anytypecode {
  after += *hello_gen_Idl
  Source_Files {
    client.cpp
  }
  Source_Files {
    testC.cpp
  }
}

