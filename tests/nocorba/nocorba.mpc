// -*- MPC -*-

project(*nocorba_gen_Idl): ridl_ostream_defaults {
  idlflags += -Scdr -SS -Scp -Sorb
  idlflags -= -Gos
  IDL_Files {
    test.idl
  }
  custom_only = 1
}

project(*nocorba_gen_Client) {
  after += *nocorba_gen_Idl
  Source_Files {
    client.cpp
  }
  Source_Files {
    testC.cpp
  }
  includes += $(TAOX11_ROOT)
}

