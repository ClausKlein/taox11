// -*- MPC -*-

project(*proxysource_gen_Idl): ridl_ostream_defaults {
  idlflags += -Gcpc -Gatc -Gath
  idlflags -= -Sa -St
  IDL_Files {
    test.idl
  }
  custom_only = 1
}

project(*proxysource_gen_Client) {
  after += *proxysource_gen_Idl
  Source_Files {
    client.cpp
  }
  Source_Files {
    testC.cpp
  }
  includes += $(TAOX11_ROOT)
  macros += TAOX11_DISABLE_INCLUDE_ANYTYPECODE
}

project(*proxysource_taox11_Client): taox11_client, taox11_anytypecode {
  exename = taox11client
  after += *proxysource_gen_Idl
  Source_Files {
    taox11client.cpp
  }
  Source_Files {
    testC.cpp
    testA.cpp
    testCP.cpp
  }
}


